<a href="{{model.url}}"><h4>{{model.companyName}}</h4></a>
<h5>{{model.jobTitle}}</h5>
<h5>Status: {{model.status}}</h5>
<h6>Deadline: {{model.deadline}}</h6>
<table>
<thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Position</th>
        <th>Email</th>
        <th>Phone Number</th>
    </tr>
</thead>
    <tr>
        <td>{{model.contact.firstName}}</td>
        <td>{{model.contact.lastName}}</td>
        <td>{{model.contact.title}}</td>
        <td>{{model.contact.email}}</td>
        <td>{{model.contact.phone}}</td>
    </tr>
</table>


